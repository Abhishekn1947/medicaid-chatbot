{
  "name": "ai-chatbot-template",
  "version": "1.0.0",
  "description": "Production-ready Angular chatbot template for integrating with any AI service",
  "keywords": ["angular", "chatbot", "ai", "template", "openai", "claude", "typescript", "material-design"],
  "author": "Abhishek Nandakumar",
  "homepage": "https://github.com/Abhishekn1947/ai-chatbot-template#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Abhishekn1947/ai-chatbot-template.git"
  },
  "bugs": {
    "url": "https://github.com/Abhishekn1947/ai-chatbot-template/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:prod": "ng build --configuration production && http-server dist/medicaid-chatbot -p 8080",
    "analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/medicaid-chatbot/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.1",
    "@angular/cli": "^19.2.1",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  }
}
